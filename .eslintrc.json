{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types" : "off",
        "react/destructuring-assignment": "off",
        "react/no-did-update-set-state": "off",
        "no-unused-expressions": "off",
        "react/no-access-state-in-setstate": "off",
        "no-console": "off"
    }
}
